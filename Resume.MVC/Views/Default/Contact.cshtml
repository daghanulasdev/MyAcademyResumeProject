@using Resume.MVC.Models
@model List<TblContact>
<div class="footer wow fadeIn" data-wow-delay="0.3s">
    <div class="container-fluid">
        <div class="container">
            @foreach(var item in Model)
            { 
            <div class="footer-info">
                <h2>@item.NameSurname</h2>
                <h3>@item.Address</h3>
                <div class="footer-menu">
                    <p>@item.Phone</p>
                    <p>@item.Email</p>
                </div>
                @Html.Action("SocialMedia")
            </div>
            }
        </div>
        <div class="container copyright">
            <p>&copy; <a href="https://github.com/daghanulasdev/MyAcademyResumeProject" target="_blank">ResumeMvc WebSite</a>, All Right Reserved | Designed By <a href="https://github.com/daghanulasdev" target="_blank">Dağhan Ulaş</a></p>
        </div>
    </div>
</div>
